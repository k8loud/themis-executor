### horiziontal scaling
POST localhost:8080/execute
Content-Type: application/json

{
  "collectionName": "kubernetes",
  "actionName": "HorizontalScalingAction",
  "params": {
    "params": {
      "resourceType": "Deployment",
      "resourceName": "nginx-deployment",
      "namespace": "default",
      "replicas": "1"
    }
  }
}

### update config map
POST localhost:8080/execute
Content-Type: application/json

{
  "collectionName": "kubernetes",
  "actionName": "UpdateConfigMapAction",
  "params": {
    "params": {
      "resourceName": "cm1",
      "namespace": "default",
      "replacements": {
        "k1": "k55",
        "v1": "v55"
      }
    }
  }
}

### delete pod
POST localhost:8080/execute
Content-Type: application/json

{
  "collectionName": "kubernetes",
  "actionName": "DeleteResourceAction",
  "params": {
    "params": {
      "resourceType": "Pod",
      "resourceName": "nginx",
      "namespace": "default",
      "gracePeriodSeconds": "60"
    }
  }
}

### delete config map
POST localhost:8080/execute
Content-Type: application/json

{
  "collectionName": "kubernetes",
  "actionName": "DeleteResourceAction",
  "params": {
    "params": {
      "resourceType": "ConfigMap",
      "resourceName": "cm1",
      "namespace": "default"
    }
  }
}
